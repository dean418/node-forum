<div class="post">
	<div>
		<h2 class="postTitle">{{this.title}}</h2>
		<p class="postContent">{{this.content}}</p>

		<div class="postFooter">
			<p>{{this.userName}}</p>
			<p>{{this.createdOn}}</p>
		</div>
	</div>
	<img class="postImage" src="http://localhost:3000/post/image/{{this.image}}">
</div>